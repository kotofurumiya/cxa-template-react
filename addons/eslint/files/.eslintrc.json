{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
      "semi": "error"
  },
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
      "jsx": true
      }
  },
  "env": {
      "browser": false,
      "node": true
  }
}
